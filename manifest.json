{
  "manifest_version": 3,
  "name": "Validador de Archivos PDF por Osani",
  "version": "1.0",
  "description": "Valida archivos PDF en una carpeta y exporta errores.",
  "permissions": ["fileSystem"],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
